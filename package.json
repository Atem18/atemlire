  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-img": "2.0.1",
    "@tailwindcss/typography": "0.5.2",
    "autoprefixer": "10.4.7",
    "html-minifier": "^4.0.0",
    "luxon": "2.4.0",
    "postcss": "8.4.13",
    "postcss-csso": "6.0.0",
    "postcss-import": "14.1.0",
    "tailwindcss": "3.0.24"
  }
}
